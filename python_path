unset PYTHONPATH
export PYTHONPATH
for i in `ls ~/code/src | grep -v "GPflow"`; do
  PYTHONPATH="$HOME/code/src/$i/src:$PYTHONPATH"
done
PYTHONPATH="$HOME/code/src/GPflow:$PYTHONPATH"
